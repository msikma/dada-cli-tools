{
  "name": "dada-cli-tools",
  "version": "1.0.3",
  "description": "Libraries for quickly building command line interfaces",
  "homepage": "https://github.com/msikma/dada-cli-tools",
  "author": "Michiel Sikma <michiel@sikma.org>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel --verbose --relative ./src/ -d ..",
    "watch": "babel --watch --verbose --relative ./src/ -d .."
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/util/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "verbose": true
  },
  "bin": {},
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "chalk": "^2.0.0",
    "cheerio": "^1.0.0-rc.2",
    "file-cookie-store-sync": "^0.1.0",
    "got": "^9.6.0",
    "lodash": "^4.17.15",
    "markdown-escape": "^1.0.2",
    "mkdirp": "^0.5.1",
    "standard-slugify": "^2.0.1",
    "string-width": "^4.2.0",
    "turndown": "^4.0.0",
    "url-parse": "^1.4.7",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  }
}
