{
  "name": "dada-cli-tools",
  "version": "1.0.0",
  "description": "Libraries for quickly building command line interfaces",
  "private": true,
  "homepage": "https://github.com/msikma/dada-cli-tools",
  "author": "Michiel Sikma <michiel@sikma.org>",
  "license": "MIT",
  "scripts": {
    "build": "babel --verbose --relative ./src/ -d ../lib",
    "watch": "babel --watch --verbose --relative ./src/ -d ../lib"
  },
  "files": [
    "src/",
    "lib/",
    "license.md",
    "package.json",
    "readme.md",
    "yarn.lock"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "verbose": true
  },
  "bin": {},
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.47",
    "@babel/core": "7.0.0-beta.47",
    "@babel/node": "7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "7.0.0-beta.47",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "file-cookie-store-sync": "^0.1.0",
    "lodash-es": "^4.17.15",
    "mkdirp": "^0.5.1"
  }
}
